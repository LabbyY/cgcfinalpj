<%= form_for @artist do |f| %>
  <p> <label>Artist Name : </label> <%= f.text_field :artistname %> </p>
  <p> <label>  Country   : </label> <%= f.text_field :country %> </p>
  <p> <label>   Genre    : </label> <%= f.text_field :genre %> </p>
  <p> <label>Introduction:</label> <%= f.text_area :introduction %> </p>
  <p> <label>   Avatar   : </label> <%= f.file_field :avatar %> </p>
  <p> <%= f.submit "Submit", class:'submit' %> </p>
<% end %>
